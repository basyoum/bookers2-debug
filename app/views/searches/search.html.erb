<script src="https://kit.fontawesome.com/b4084b3cf0.js" crossorigin="anonymous"></script>

<% if @range == 'user' %>
 <h3>Users search for "<%= @search %> "</h3>
  <%= render 'users/index', users: @records %>
<% elsif @range == 'book' %>
 <h3>Boks search for "<%= @search %>"</h3>
  <%= render 'books/index', books: @records %>
<% end %>